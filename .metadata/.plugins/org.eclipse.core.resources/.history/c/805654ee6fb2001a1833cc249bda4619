class Machine{
	private String name;
	private int code;
	
	public Machine() {
		System.out.println("Constructor running");
	}
	
	public Machine(String name) {
		System.out.println("Second constructor");
		this.name=name;
	}
}

public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Machine machine1 = new Machine();
		
		new Machine();
		
	}

}
